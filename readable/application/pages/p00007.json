       ,"SQL Query":"select \"DEPTNO\",\n    null LINK_CLASS,\n    apex_page.get_url(p_items =\u003E \u0027P7_DEPTNO\u0027, p_values =\u003E \"DEPTNO\") LINK,\n    null ICON_CLASS,\n    null LINK_ATTR,\n    null ICON_COLOR_CLASS,\n    case when coalesce(:P7_DEPTNO,\u00270\u0027) = \"DEPTNO\"\n      then \u0027is-active\u0027 \n      else \u0027 \u0027\n    end LIST_CLASS,\n    (substr(\"DEPTNO\", 1, 50)||( case when length(\"DEPTNO\") \u003E 50 then \u0027...\u0027 else \u0027\u0027 end )) LIST_TITLE,\n    (substr(\"LOC\", 1, 50)||( case when length(\"LOC\") \u003E 50 then \u0027...\u0027 else \u0027\u0027 end )) LIST_TEXT,\n    null LIST_BADGE\nfrom \"DEPT\" x\nwhere (:P7_SEARCH is null\n        or upper(x.\"DEPTNO\") like \u0027%\u0027||upper(:P7_SEARCH)||\u0027%\u0027\n        or upper(x.\"LOC\") like \u0027%\u0027||upper(:P7_SEARCH)||\u0027%\u0027\n    )\norder by \"DEPTNO\""
