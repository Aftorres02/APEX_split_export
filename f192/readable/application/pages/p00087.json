 ,"Processes":[
    {
      "id":"15933297132150577676"
     ,"Identification":{
        "Name":"resequence display sequence"
       ,"Type":"Execute Code"
       ,"Execution Chain":"None"
      }
     ,"Source":{
        "Location":"Local Database"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"declare\n  c int := 0;\nbegin\n  for c1 in (select id \n             from EBA_CUST_REF_PHASE \n             order by nvl(display_sequence,10), DESCRIPTION) loop\n      c := c + 10;\n      update EBA_CUST_REF_PHASE set display_sequence = c \n      where id = c1.id and (display_sequence is null or display_sequence != c);\n  end loop;\nend;\n  "
      }
     ,"Execution":{
        "Sequence":10
       ,"Point":"Processing"
       ,"Run Process":"Once Per Page Visit (default)"
      }
     ,"Success Message":{
        "Success Message":"Display sequence values have been resequenced."
      }
