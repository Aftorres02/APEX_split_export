     ,"Source":{
        "Location":"Local Database"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Code":"if instr(:APP_USER,\u0027@\u0027) \u003E 0 then\n    eba_cust_fw.set_preference_value (\u0027USERNAME_FORMAT\u0027,\u0027EMAIL\u0027);\nelse\n    eba_cust_fw.set_preference_value (\u0027USERNAME_FORMAT\u0027,\u0027STRING\u0027);\nend if;"
      }
